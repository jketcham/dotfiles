!! URxvt settings

URxvt.font:             xft:Hack:size=16
URxvt*scrollBar:        false
URxvt*scrollBar_right:  true
URxvt*scrollColor:      #000000

URxvt.perl-ext-common: default,clipboard,tabbed
URxvt.clipboard.copycmd: xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.keysym.Shift-Control-C: perl:clipboard:copy

URxvt.iso14755: false
URxvt.iso14755_52: false

!! some HiDPI settings
Xcursor.size:           50
Xft.autohint: 		      0
Xft.lcdfilter: 		      lcddefault
Xft.hintstyle: 		      hintfull
Xft.hinting: 		        1
Xft.antialias: 		      1
Xft.rgba: 		          rgb
Xft.dpi:                90

! Dracula Xresources palette
*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
*.color3:     #F1FA8C
*.color11:    #F4F99D
*.color4:     #BD93F9
*.color12:    #CAA9FA
*.color5:     #FF79C6
*.color13:    #FF92D0
*.color6:     #8BE9FD
*.color14:    #9AEDFE
*.color7:     #BFBFBF
*.color15:    #E6E6E6

 URxvt.tabbed.tabbar-fg: <colour-index, default 3>
 URxvt.tabbed.tabbar-bg: <colour-index, default 0>
 URxvt.tabbed.tab-fg:    <colour-index, default 0>
 URxvt.tabbed.tab-bg:    <colour-index, default 1>
